	
<form class="add_pic" action="/pictures" method="post">
	Enter URL: <input type="text" name="sample_text_field">
	Enter a comment: <input type="text" name="comment_text_field">
	<%= submit_tag "Start the Magic"%>
</form>



<ul>
	<% @picture_urls.each do |picture| %>
		<div class="image_frame">
			<li><img src="<%= picture.url %>"></li>
			<p class="comment_box"><%= picture.comments %></p>
		</div>
	<% end %>
</ul>







